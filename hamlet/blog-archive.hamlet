<ul>
    $forall year <- archive
        <li>
            #{show $ fst year}
            <ul>
                $forall month <- snd year
                    <li>
                        #{prettyMonth $ fst month}
                        <ul>
                            $forall entry <- snd month
                                <li>
                                    <a title=#{eDate entry} href=@{eLink entry}>#{eTitle entry}
